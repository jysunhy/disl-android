<?xml version="1.0" encoding="UTF-8"?>
<config>
	<default-dislclass></default-dislclass>
	<default-observed>false</default-observed>
	<dexes> 
		<dex>
			<name>ImplicitFlows_ImplicitFlow4.apk</name>
			<bypass>false</bypass>
			<dislclass>output/build/analysis/ch/usi/dag/cc/disl/DiSLClass.class</dislclass>
		</dex>
		<!-- optional -->
		<!-- instrument Activity.onPause so that we can get result at specified time, when user get back to home-->
		<!-- of course we can configure our analysis to print coverage log on the fly, we do this here to get a summary for coverage for a period -->
		<dex>
			<name>framework.jar</name>
			<isbootstrap>true</isbootstrap>
			<dislclass>output/build/analysis/ch/usi/dag/cc/disl/FrameworkDiSLClass.class</dislclass>
		</dex>
	</dexes>
	<procs>
		<!-- in this case, only the target program is observed -->
		<proc>
			<procname>de.ecspride.implicitflows</procname>
			<isobserved>true</isobserved>
		</proc>
	</procs>
</config>
